
  <launch>


  <rosparam file="$(find realsense_explorer_description)/config/differential_controller.yaml" command="load"/>

<node name="controller_spawner" pkg="controller_manager" type="spawner" ns="realsense_explorer_control" output="screen" args="diff_drive_controller"/>

  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="115200"/>
    </node>

    
  </launch>